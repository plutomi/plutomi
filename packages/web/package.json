{
  "name": "@plutomi/web",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "next dev",
    "lint": "yarn eslint --ext .ts,.tsx .",
    "lint:fix": "yarn eslint --ext .ts,.tsx --fix .",
    "test": "jest",

    "pages:build": "npx @cloudflare/next-on-pages@1",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages@1 --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-10-16 --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.17",
    "@hookform/resolvers": "^3.1.1",
    "@tabler/icons-react": "^2.17.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query": "^4.29.7",
    "@tanstack/react-query-devtools": "^4.29.7",
    "@tiptap/extension-link": "^2.0.3",
    "axios": "^1.3.6",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "next": "^13.5.4",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.8.0",
    "remove": "^0.1.5",
    "tailwind-merge": "^1.14.0",
    "zustand": "^4.3.8"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.12",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.2.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.2",
    "@cloudflare/next-on-pages": "^1.7.1",
    "eslint-plugin-next-on-pages": "^1.7.1",
    "vercel": "^32.5.0"
  }
}
